<div>
    <h1>Your Favorite Artists</h1>
    <% artists.forEach(favoriteArtist => { %>
        <div class="artistPage">
            <ul>
                <li><%= favoriteArtist.name %></li>
            </ul>
            <p><img src="<%= favoriteArtist.image %>" alt="image" class="images"></p>
            <p><a href="<%= favoriteArtist.webURL %>">Visit their webpage</a></p>      
        </div>
    <% }) %>
</div>
<div class= back-to-profile>
    <a href="/profile">Back to Profile</a>
</div>